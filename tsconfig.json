{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "types": ["jquery", "vite/client", "@intlify/unplugin-vue-i18n/messages", "vitest/globals", "tooltipster"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": ["esnext", "dom", "dom.iterable"]
  },
  "include": [
    "**/*.js",
    "**/*.ts",
    "**/*.tsx",
    "**/*.vue"
, "vite.config.mts"  ],
  "exclude": ["node_modules"]
}
